<UserControl x:Class="StreamControl.Views.StartupDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
             prism:ViewModelLocator.AutoWireViewModel="True" FocusManager.FocusedElement="{Binding ElementName=txt}">
    <StackPanel Margin="4">
        <ItemsControl prism:RegionManager.RegionName="Startup">
            
        </ItemsControl>
        <Button Content="Ok" Command="{Binding OkCommand}" Margin="0,8,0,4" IsDefault="True"/>
    </StackPanel>
    
</UserControl>
